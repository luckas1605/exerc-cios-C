
#include <stdio.h>

int
main ()
{
  int nmr1, nmr2, ordem;

  printf ("digite dois numeros:\n ");
  scanf ("%d", &nmr1);
  scanf ("%d", &nmr2);

  while (nmr1 != nmr2)
    {

      if (nmr1 < nmr2)
	{

	  printf ("CRECENTE\n");
	}
      else
	{
	  printf ("DRECRECENTE\n");
	}
      printf ("digite outros dois numeros:\n ");
      scanf ("%d", &nmr1);
      scanf ("%d", &nmr2);

    }



  return 0;
}
